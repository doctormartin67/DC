# This is a small awk file to manipulate the sharedStrings.xml file that is used to extract
# the strings that are in the excel file
BEGIN {
    RS = "(<c r=\")|(</v></c>)";
    FS = "(<v>)|( aca=\"false\")";
}
NF == 2 {print $1 "<v>" $NF "</v>"}
NF == 3 {print $1 $2 "<v>" $NF "</v>"} #This line gets run if aca = "false" is found

